#include "main.h"
/**
 * _printf - function that print any thing;
 *
 * @format: argument
 *
 * Return: number of character printed.
 */

int _printf(const char *format, ...);
{
	int num_prnt = 0;
	
	if (!format)
		return (-1);

	va_list arglist;

	va_start(arglist, format);
	const char *p == format;
	int (*f)(va_list);

	while (*p != \0)
	{
		if (*p == '%')
		{
			f = check_func(*(p+1));
			if (f != NULL)
			{
			num_prnt += f(arglist);
			p += 2;
			continue;
			}
			if (*(p + 1) == \0)
					break;
			if (*(p + 1))
			{
				num_prnt += _putchar(*(p + 1));
				p += 2;
				continue;
			}
			}
			else
			{
			num_prnt += _putchar(*p);
			continue;
			}
			p++;

			}

			va_end(arglist);
			return (num_prnt);
}

